<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<packageSources>
		<add key="local" value="./packages"/>
		<add key="github" value="https://nuget.pkg.github.com/byr0n3/index.json"/>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3"/>
	</packageSources>
	<packageSourceMapping>
		<packageSource key="local">
			<package pattern="Engine.WebGPU"/>
			<package pattern="Engine.WebGPUd"/>
			<package pattern="Engine.GLFW"/>
			<package pattern="Engine.GLFWd"/>
		</packageSource>
		<packageSource key="github">
			<package pattern="Elegance.*"/>
		</packageSource>
		<packageSource key="nuget.org">
			<package pattern="*"/>
		</packageSource>
	</packageSourceMapping>
</configuration>
